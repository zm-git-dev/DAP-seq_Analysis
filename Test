module load SAMtools/1.10-GCC-8.2.0-2.31.1

sort -O 'bam' -o SRX3786374_sorted.bam SRX3786374.sam ## log 생성해놓기!! 
samtools view -q 30 SRX3786374_sorted.bam -o SRX3786374.sorted.filtered.bam


